}

% TESIS - tesis de maestria.
%  obl:   author, title, school, year.
%  ops:   type (eg. "diploma thesis"), address, month, note.
%
@mastersthesis{tes:tesis,
    author			= {Nombre autor},
    title			= {Titulo de tesis},
    school			= {Nombre de universidad},
    type			= {Tesis de diplomado*},
    address			= {Dirección*},
    year			= {20XX},
    month			= {mes-nom*},
    note			= {Notas y observaciones*},
    language        = {spanish},
}

% OPCIONAL - Plantilla útil para otros tipos de publicación.
%  obl:   none
%  ops:   author, title, howpublished, month, year, note.
%
@misc{opc:opcional,
    author			= {Autor*},
    title			= {Titulo*},
    howpublished	= {Como de publico*},
    year			= {20XX*},
    month			= {mes-nom*},
    note			= {Notas y observaciones*},
    language        = {spanish},
}

% WEB - Ejemplo
%
@misc{web:web,
